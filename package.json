{
  "name": "twich-clip-roop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "preview": "vite preview",
    "prepare": "husky"
